<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <title>{{title}}</title>
    <link href="/stylesheets/common-css/demo.min.css?1674944402" rel="stylesheet" />
    <link href="/stylesheets/common-css/tabler-flags.min.css?1674944402" rel="stylesheet" />
    <link href="/stylesheets/common-css/tabler-payments.min.css?1674944402" rel="stylesheet" />
    <link href="/stylesheets/common-css/tabler-vendors.min.css?1674944402" rel="stylesheet" />
    <link href="/stylesheets/common-css/tabler.min.css?1674944402" rel="stylesheet" />
    <style>
        @import url('https://rsms.me/inter/inter.css');

        :root {
            --tblr-font-sans-serif: 'Inter Var', -apple-system, BlinkMacSystemFont, San Francisco, Segoe UI, Roboto, Helvetica Neue, sans-serif;
        }

        body {
            font-feature-settings: "cv03", "cv04", "cv11";
        }
    </style>
</head>

<body>
    <script src="/javascripts/index-js/demo-theme.min.js?1674944402"></script>
    <div class="page container-fluid">
        <!-- Navbar -->

        <div class="row g-2 align-items-center mt-3">
            <div class="col">
                <!-- Page pre-title -->
                <div class="page-pretitle">
                    {{userDetails.firstName}}
                </div>
                <h2 class="page-title">
                    {{userDetails.lastName}}
                </h2>
            </div>
            <div class="col-auto ms-auto d-print-none">
                <div class="btn-list">
                    <div class="input-group" style="width : 500px; margin-left: 320px">
                        <input type="search" class="form-control rounded" id="searchBarValue" placeholder="Search by FirstName LastName Email" aria-label="Search"
                            aria-describedby="search-addon">
                    </div>
                    <span class="d-none d-sm-inline">
                        <a href="/" class="btn sort" data-sort="title">
                            Go back
                        </a>
                    </span>
                    <li class="nav-item dropdown ">
                        <a class="nav-link dropdown-toggle " href="#navbar-base" data-bs-toggle="dropdown"
                            data-bs-auto-close="outside" role="button" aria-expanded="false">
                            <span class="nav-link-icon d-md-none d-lg-inline-block">
                                <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-app-window"
                                    width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor"
                                    fill="none" stroke-linecap="round" stroke-linejoin="round">
                                    <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                                    <path
                                        d="M3 5m0 2a2 2 0 0 1 2 -2h14a2 2 0 0 1 2 2v10a2 2 0 0 1 -2 2h-14a2 2 0 0 1 -2 -2z">
                                    </path>
                                    <path d="M6 8h.01"></path>
                                    <path d="M9 8h.01"></path>
                                </svg>
                            </span>
                            <span class="nav-link-title  ">
                                SortBy
                            </span>
                        </a>
                        <div class="dropdown-menu">
                            <div class="dropdown-menu-columns">
                                <div class="dropdown-menu-column">
                                    <a class="dropdown-item postSort" href="javascript:void(0)" id="dateSort"
                                        data-id="sortbyDate" data-sort-order="1">
                                        <b>Registration Date</b>
                                    </a>
                                </div>
                            </div>
                        </div>
                    </li>
                </div>
            </div>
            <div class="container-fluid bodyDiv" style="margin-top: 50px; margin-left : 500px">
                {{> userList}}
            </div>
            <footer class="footer footer-transparent d-print-none">
                <div class="container-xl">
                    <div class="row text-center align-items-center flex-row-reverse">
                        <div class="col-lg-auto ms-lg-auto">
                            <ul class="list-inline list-inline-dots mb-0">
                                <li class="list-inline-item"><a href="./docs/" class="link-secondary">Documentation</a>
                                </li>
                                <li class="list-inline-item"><a href="./license.html" class="link-secondary">License</a>
                                </li>
                                <li class="list-inline-item"><a href="https://github.com/tabler/tabler" target="_blank"
                                        class="link-secondary" rel="noopener">Source code</a></li>
                                <li class="list-inline-item">
                                    <a href="https://github.com/sponsors/codecalm" target="_blank"
                                        class="link-secondary" rel="noopener">
                                        <!-- Download SVG icon from http://tabler-icons.io/i/heart -->
                                        <svg xmlns="http://www.w3.org/2000/svg"
                                            class="icon text-pink icon-filled icon-inline" width="24" height="24"
                                            viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none"
                                            stroke-linecap="round" stroke-linejoin="round">
                                            <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                                            <path
                                                d="M19.5 12.572l-7.5 7.428l-7.5 -7.428a5 5 0 1 1 7.5 -6.566a5 5 0 1 1 7.5 6.572" />
                                        </svg>
                                        Sponsor
                                    </a>
                                </li>
                            </ul>
                        </div>
                        <div class="col-12 col-lg-auto mt-3 mt-lg-0">
                            <ul class="list-inline list-inline-dots mb-0">
                                <li class="list-inline-item">
                                    Copyright &copy; 2023
                                    <a href="." class="link-secondary">Tabler</a>.
                                    All rights reserved.
                                </li>
                                <li class="list-inline-item">
                                    <a href="./changelog.html" class="link-secondary" rel="noopener">
                                        v1.0.0-beta17
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </footer>
        </div>
</body>

</html>