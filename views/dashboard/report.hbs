<div class="card">
    <div class="table-responsive">
        <table class="table table-vcenter card-table table-striped">
            <thead>
                <tr>
                    <th>Date</th>
                    <th>Total Created Post</th>
                    <th>Total SavedPost</th>
                </tr>
            </thead>
            <tbody>
                {{#each statisticData}}
                <tr>
                    <td>{{dateFormat createdOn "YYYY-MM-DD HH:mm:ss"}}</td>
                    <td class="text-muted">
                        {{totalCreatedPost}}
                    </td>
                    <td class="text-muted">{{totalSavedPost}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</div>