<div class="page-body">
    {{#if postData}}
    {{#each postData}}
    {{#each postdata}}
    <div class="container-xl" style="margin-left: 700px">
        <div class="row row-cards ">
            <div class="col-sm-6 col-lg-4 grandParent">
                <div class="card card-sm ">
                    <img src="/images/posts/{{postPath}}" class="card-img-top" style="height: 300px;">
                    <div class="card-body">
                        <div class="d-flex align-items-center">
                            <span class="avatar me-3 rounded"
                                style="background-image: url(/images/users/{{../profilePath}})"></span>
                            <div>
                                <div>{{../firstName}} {{../lastName}}</div>
                                <div class="text-muted">{{dateFormat createdOn "MMMM Do YYYY"}}</div>
                            </div>
                            <div class="ms-auto d-flex">
                                {{#inArray ../../savedPost this._id}}
                                <a href="javascript:void(0)" class="text-muted" data-title="Unsave">
                                    <div class="savedPostDiv save-post" data-id="{{_id}}">
                                        <svg xmlns="http://www.w3.org/2000/svg"
                                            class="icon icon-tabler icon-tabler-heart-filled" width="24" height="24"
                                            viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none"
                                            stroke-linecap="round" stroke-linejoin="round">
                                            <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                                            <path
                                                d="M6.979 3.074a6 6 0 0 1 4.988 1.425l.037 .033l.034 -.03a6 6 0 0 1 4.733 -1.44l.246 .036a6 6 0 0 1 3.364 10.008l-.18 .185l-.048 .041l-7.45 7.379a1 1 0 0 1 -1.313 .082l-.094 -.082l-7.493 -7.422a6 6 0 0 1 3.176 -10.215z"
                                                stroke-width="0" fill="currentColor"></path>
                                        </svg>
                                    </div>
                                </a>
                                {{else}}
                                {{#compare ../_id "!=" ../../userDetails._id }}
                                <a href="javascript:void(0)" class="text-muted" data-title="Save">
                                    <div class="savedPostDiv save-post" data-id="{{_id}}">
                                        <svg xmlns="http://www.w3.org/2000/svg"
                                            class="icon icon-tabler icon-tabler-heart" width="24" height="24"
                                            viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none"
                                            stroke-linecap="round" stroke-linejoin="round">
                                            <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                                            <path
                                                d="M19.5 12.572l-7.5 7.428l-7.5 -7.428a5 5 0 1 1 7.5 -6.566a5 5 0 1 1 7.5 6.572">
                                            </path>
                                        </svg>
                                    </div>
                                </a>
                                {{/compare}}
                                {{/inArray}}
                                {{#compare ../_id "===" ../../userDetails._id }}
                                <a href="javascript: void(0)" class="text-muted" id="editBtn" title="Edit Post"
                                    data-bs-toggle="modal" data-bs-target="#modal-editPost" data-id="{{_id}}">

                                    <svg xmlns="http://www.w3.org/2000/svg"
                                        class="icon icon-tabler icon-tabler-pencil-minus" width="24" height="24"
                                        viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none"
                                        stroke-linecap="round" stroke-linejoin="round">
                                        <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                                        <path d="M8 20l10.5 -10.5a2.828 2.828 0 1 0 -4 -4l-10.5 10.5v4h4z"></path>
                                        <path d="M13.5 6.5l4 4"></path>
                                        <path d="M16 18h4"></path>
                                    </svg>
                                </a>

                                <a href="javascript:void(0)" class="text-muted" data-archive="unarchive">
                                    <div class="archivePostDiv" data-id="{{_id}}">
                                        <svg xmlns="http://www.w3.org/2000/svg"
                                            class="icon icon-tabler icon-tabler-exposure" width="24" height="24"
                                            viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none"
                                            stroke-linecap="round" stroke-linejoin="round">
                                            <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                                            <path
                                                d="M4 4m0 2a2 2 0 0 1 2 -2h12a2 2 0 0 1 2 2v12a2 2 0 0 1 -2 2h-12a2 2 0 0 1 -2 -2z">
                                            </path>
                                            <path d="M4.6 19.4l14.8 -14.8"></path>
                                            <path d="M7 9h4m-2 -2v4"></path>
                                            <path d="M13 16l4 0"></path>
                                        </svg>
                                    </div>
                                </a>
                                {{/compare}}
                            </div>
                        </div><br>
                        <div><span class="text-muted">Title : </span> {{title}}</div>
                        <div><span class="text-muted">Description : </span>
                            {{description}}</div>
                    </div>
                </div>
            </div>
        </div>
    </div><br><br>
    {{/each}}
    {{/each}}

    <div class="d-flex">
        <ul class="pagination ms-auto">
            <li class="page-item disabled">
                <a class="page-link" href="#" tabindex="-1" aria-disabled="true">
                    <svg xmlns="http://www.w3.org/2000/svg" class="icon" width="24" height="24" viewBox="0 0 24 24"
                        stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round"
                        stroke-linejoin="round">
                        <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                        <path d="M15 6l-6 6l6 6"></path>
                    </svg>
                    prev
                </a>
            </li>

            {{#each pages}}
            <li class="page-item {{this}}"><a class="page-link pages" href="javascript:void(0)"
                    data-page-value="{{this}}">{{this}}</a></li>
            {{/each}}
            <li class="page-item">
                <a class="page-link" href="#">
                    next
                    <svg xmlns="http://www.w3.org/2000/svg" class="icon" width="24" height="24" viewBox="0 0 24 24"
                        stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round"
                        stroke-linejoin="round">
                        <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                        <path d="M9 6l6 6l-6 6"></path>
                    </svg>
                </a>
            </li>
        </ul>
    </div>

    {{else}}
    <h2 style="margin: 0px 500px 5px 745px;">No Post Available</h2>
    <div class="empty-action" style="margin: 0px 500px 500px 750px;">
        <a href="/" class="btn btn-primary">
            <svg xmlns="http://www.w3.org/2000/svg" class="icon" width="24" height="24" viewBox="0 0 24 24"
                stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                <path d="M5 12l14 0"></path>
                <path d="M5 12l6 6"></path>
                <path d="M5 12l6 -6"></path>
            </svg>
            Take me home
        </a>
    </div>
    {{/if}}
</div>