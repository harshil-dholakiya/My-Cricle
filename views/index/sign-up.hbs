<html lang="en">

<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover" />
  <meta http-equiv="X-UA-Compatible" content="ie=edge" />
  <title>Sign up - Tabler - Premium and Open Source dashboard template with responsive and high quality UI.</title>
  <!-- CSS files -->
  {{#section 'style'}}
  <link href="stylesheets/common-css/demo.min.css" rel="stylesheet" />
  <link href="stylesheets/common-css/tabler-flags.min.css" rel="stylesheet" />
  <link href="stylesheets/common-css/tabler-payments.min.css" rel="stylesheet" />
  <link href="stylesheets/common-css/tabler-vendors.min.css" rel="stylesheet" />
  <link href="stylesheets/common-css/tabler.min.css" rel="stylesheet" />
  {{/section}}
  <style>
    @import url('https://rsms.me/inter/inter.css');

    :root {
      --tblr-font-sans-serif: 'Inter Var', -apple-system, BlinkMacSystemFont, San Francisco, Segoe UI, Roboto, Helvetica Neue, sans-serif;
    }

    body {
      font-feature-settings: "cv03", "cv04", "cv11";
    }
  </style>

</head>

<body class=" d-flex flex-column">
  <script src="javascripts/index-js/demo-theme.min.js"></script>
  <div class="page page-center">
    <div class="container container-tight py-4">
      <div class="text-center mb-4">
        <a href="." class="navbar-brand navbar-brand-autodark"><img src="/logo.png" alt="Logo"
            style="height: 50px; width : 60px"></a>
      </div>
      <form class="card card-md" action="sign-up-data" method="post" autocomplete="off" novalidate name="signup"
        id="signup">
        <div class="card-body">
          <h2 class="card-title text-center mb-4">Create new account</h2>
          <div class="mb-3">
            <label class="form-label"> First Name</label>
            <input type="text" class="form-control" name="firstName" placeholder="Enter name" id="fname">
          </div>
          <div class="mb-3">
            <label class="form-label">Last Name</label>
            <input type="text" class="form-control" name="lastName" placeholder="Enter name" id="lname">
          </div>
          <div class="mb-3">
            <label class="form-label">Email address</label>
            <input type="text" class="form-control" name="email" placeholder="Enter email" id="email">
          </div>

          <label class="form-label">Gender</label>
          <div class="form-check">
            <input class="form-check-input" type="radio" name="gender" id="radiobtn1" class="radiobtn" value="male">
            <label class="form-check-label" for="flexRadioDefault1">
              Male
            </label>
          </div>
          <div class="form-check">
            <input class="form-check-input" type="radio" name="gender" id="radiobtn2" class="radiobtn" value="female">
            <label class="form-check-label" for="flexRadioDefault2">
              Female
            </label>
          </div>
          <div class="mb-3">
            <label class="form-label">Account Type</label>
            <div class="form-check">
              <input class="form-check-input" type="radio" name="account" id="public" class="account" value="public">
              <label class="form-check-label" for="flexRadioDefault1">
                Public
              </label>
            </div>
            <div class="form-check">
              <input class="form-check-input" type="radio" name="account" id="private" class="account" value="private">
              <label class="form-check-label" for="flexRadioDefault2">
                Private
              </label>
            </div>
          </div>

          <div class="mb-3">
            <label class="form-label">Password</label>
            <div class="input-group input-group-flat">
              <input type="password" class="form-control" placeholder="Password" name="password" autocomplete="off"
                id="password">
              <span class="input-group-text">
                <a href="#" class="link-secondary" title="Show password" data-bs-toggle="tooltip">
                </a>
              </span>
            </div>
          </div>
          <div class="mb-3">
            <label class="form-label">Confrim Password</label>
            <div class="input-group input-group-flat">
              <input type="password" class="form-control" placeholder="Password" autocomplete="off"
                name="confrimPassword">
              <span class="input-group-text">
                <a href="#" class="link-secondary" title="Show password" data-bs-toggle="tooltip">
                </a>
              </span>
              <div class="errorTxt"></div>
            </div>
          </div>
          <div class="mb-3">
            <label class="form-check">
              <input type="checkbox" class="form-check-input" />
              <span class="form-check-label">Agree the <a href="./terms-of-service.html" tabindex="-1">terms and
                  policy</a>.</span>
            </label>
          </div>
          <div class="form-footer">
            <button type="submit" class="btn btn-primary w-100 signup">Create new account</button>
          </div>
        </div>
      </form>
      <div class="text-center text-muted mt-3">
        Already have account? <a href="/sign-in" tabindex="-1">Sign in</a>
      </div>
    </div>
  </div>

</body>
{{#section 'script'}}
<script src="javascripts/jquery/jquery.min.js"></script>
<script src="https://cdn.jsdelivr.net/jquery.validation/1.16.0/jquery.validate.min.js"></script>
<script src="javascripts/custome-js/validate.js"></script>
{{/section}}

</html>